<attribute id="level_np_form" name="NP_Form" type="nominal_button" text="Choose noun phrase form">
  <value id="value_np_form_none" name="none"/>
  <value id="value_np_form_ne" name="named_entity" next="level_corefclass"/>
  <value id="value_np_form_def-np" name="def-np" next="level_corefclass"/>
